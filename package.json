{
   "name": "electron-vite-react",
   "version": "1.0.3",
   "main": "dist-electron/main/index.js",
   "description": "Beta version",
   "license": "MIT",
   "private": false,
   "author": "project Automations",
   "debug": {
      "env": {
         "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
      }
   },
   "scripts": {
      "dev": "vite",
      "build": "tsc && vite build && electron-builder",
      "publish": "tsc && vite build && electron-builder -p always",
      "preview": "vite preview",
      "pree2e": "vite build --mode=test",
      "e2e": "playwright test"
   },
   "build": {
      "publish": [
         {
            "provider": "github",
            "owner": "RegnaR19",
            "repo": "electron-vite-react"
         }
      ],
      "nsis": {
         "oneClick": false,
         "perMachine": false,
         "allowElevation": true
      }
   },
   "devDependencies": {
      "@playwright/test": "^1.31.0",
      "@types/react": "^18.0.28",
      "@types/react-dom": "^18.0.11",
      "@vitejs/plugin-react": "^3.1.0",
      "electron": "^22.3.1",
      "electron-builder": "^23.6.0",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "sass": "^1.58.3",
      "typescript": "^4.9.5",
      "vite": "^4.1.4",
      "vite-electron-plugin": "^0.8.2",
      "vite-plugin-electron": "^0.11.1",
      "vite-plugin-electron-renderer": "^0.12.1"
   },
   "engines": {
      "node": "^14.18.0 || >=16.0.0"
   },
   "dependencies": {
      "@emotion/react": "^11.10.6",
      "@mantine/carousel": "^5.10.5",
      "@mantine/core": "^5.10.5",
      "@mantine/dates": "^5.10.5",
      "@mantine/dropzone": "^5.10.5",
      "@mantine/form": "^5.10.5",
      "@mantine/hooks": "^5.10.5",
      "@mantine/modals": "^5.10.5",
      "@mantine/notifications": "^5.10.5",
      "@mantine/nprogress": "^5.10.5",
      "@mantine/prism": "^5.10.5",
      "@mantine/spotlight": "^5.10.5",
      "@mantine/tiptap": "^5.10.5",
      "@tabler/icons": "^1.119.0",
      "@tabler/icons-react": "^2.6.0",
      "@tiptap/extension-link": "^2.0.0-beta.220",
      "@tiptap/react": "^2.0.0-beta.220",
      "@tiptap/starter-kit": "^2.0.0-beta.220",
      "axios": "^1.3.4",
      "cors": "^2.8.5",
      "dayjs": "^1.11.7",
      "electron-log": "^5.0.0-beta.16",
      "electron-updater": "^5.3.0",
      "embla-carousel-react": "^7.0.9",
      "react-redux": "^8.0.5",
      "react-router-dom": "^6.8.2",
      "redux": "^4.2.1"
   }
